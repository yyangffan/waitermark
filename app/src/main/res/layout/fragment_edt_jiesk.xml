                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_corner_kuang"
                android:drawableRight="@drawable/icon_xiala"
                android:ellipsize="end"
                android:hint="请选择开户行"
                android:lines="1"
                android:paddingLeft="@dimen/dp_5"
                android:paddingTop="@dimen/dp_5"
                android:paddingRight="@dimen/dp_5"
                android:paddingBottom="@dimen/dp_5"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/item_lookjies_yuliuphone"
                app:layout_constraintStart_toStartOf="@+id/item_lookjies_yuliuphone"
