                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/item_lookjies_kaihuren"
                app:layout_constraintStart_toStartOf="@+id/item_lookjies_kaihuren"
                app:layout_constraintTop_toBottomOf="@+id/item_lookjies_kaihuren"/>

            <TextView
